<?xml version="1.0" encoding="UTF-8"?>
<data formatter="SimpleXMLFormatter">
    <types>
        <object class="Profile" base="Profile">
            <attribute name="objective" />
            <attribute name="firstName" />
            <attribute name="lastName" />
            <attribute name="status" />
            <attribute name="defaultEmail" />
            <attribute name="defaultPhone" />
            <attribute name="defaultAddress" />
        </object>
        <object class="DetailedProfile" base="Profile">
            <attribute name="objective" />
            <attribute name="firstName" />
            <attribute name="lastName" />
            <attribute name="status" />
            <attribute name="defaultEmail" />
            <attribute name="defaultPhone" />
            <attribute name="defaultAddress" />
            <attribute name="addresses" /> 
            <attribute name="media" />
            <attribute name="languages" />
            <attribute name="activities" />
        </object>
        <object class="Education" base="Degree">
            <attribute name="school" />
            <attribute name="status" />
            <attribute name="startDate" />
            <attribute name="endDate" />
            <attribute name="description" />
            <attribute name="location" />
        </object>
        <object class="Skill" base="Skill">
            <attribute name="name" />
            <attribute name="type" />
        </object>
        <object class="Experience" base="Experience">
            <attribute name="position" />
            <attribute name="description" />
            <attribute name="department" />
            <attribute name="startdate" />
            <attribute name="endDate" />
            <attribute name="isCurrent" />
            <attribute name="activities" />
        </object>
        <object class="Media" base="Media">
            <attribute name="type" />
            <attribute name="name" />
            <attribute name="value" />
        </object>
        <object class="Address" base="UserAddress">
            <attribute name="street1" />
            <attribute name="street2" />
            <attribute name="city" />
            <attribute name="province" />
            <attribute name="postalCode" />
            <attribute name="country" />
        </object>
        <object class="Media" base="Media">
            <attribute name="type" />
            <attribute name="name" />
            <attribute name="value" />
        </object>
        <object class="Language" base="Language">
            <attribute name="name" />
        </object>
        <object class="Activity" base="Activity">
            <attribute name="description" />
        </object>
    </types>
    <interfaces scope="resumes">
        <interface name="profile" type="Profile" />
        <interface name="detail" type="DetailedProfile" />
        <interface name="educations" type="Education" collection="true" />
        <interface name="skills" type="Skill" collection="true" />
        <interface name="skills#technical" type="Skill" collection="true" />
        <interface name="skills#soft" type="Skill" collection="true" />
        <interface name="experiences" type="Skill" collection="true" />
        <interface name="experiences#work-history" type="Experience" collection="true" />
        <interface name="experiences#volunteer" type="Experience" collection="true" />
    </interfaces>
    <interfaces scope="user">
        <interface name="detail" type="DetailedProfile" collection="true" />
        <interface name="educations" type="Education" collection="true" />
        <interface name="skills" type="Skill" collection="true" />
        <interface name="skills#technical" type="Skill" collection="true" />
        <interface name="skills#soft" type="Skill" collection="true" />
        <interface name="experiences" type="Skill" collection="true" />
        <interface name="experiences#work-history" type="Experience" collection="true" />
        <interface name="experiences#volunteer" type="Experience" collection="true" />
    </interfaces>
</data>
